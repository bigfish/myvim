
package com.bluecoat.models {

	public class ConnectivityModel 
	{
		public static var _instance:ConnectivityModel;

		public function ConnectivityModel(single:SingletonEnforcer) {

		}
		/**
		 * ensure that this is a Singleton
		 */
		public static function getInstance():ConnectivityModel {
			if (_instance == null)
			{
				_instance = new ConnectivityModel(new SingletonEnforcer());
			}
			return _instance;
		}
	}
}
// Internal constructor parameter used
// to enforce the singleton pattern
class SingletonEnforcer{}
	
